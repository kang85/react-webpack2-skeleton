{
  "name": "react-webpack2-skeleton",
  "version": "1.0.0",
  "description": "Get started with React with Webpack2",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "webpack:lambda": "webpack --config config/webpack/config.lambda.js",
    "lambda": "webpack --config config/webpack/config.lambda.js && node-lambda run",
    "start:dev": "webpack-dev-server --config config/webpack/config.dev.js",
    "start": "node server",
    "build": "rimraf build && webpack --config config/webpack/config.js",
    "build:server": "webpack --config config/webpack/config.server.js",
    "deploy": "npm run build && npm run webpack:lambda && sls deploy -w"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-app": "^2.1.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^2.28.0",
    "ignore-loader": "^0.1.2",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.2",
    "react-hot-loader": "next",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "@types/lodash": "^4.14.71",
    "babel-node-lambda": "^1.1.1",
    "express": "^4.15.0",
    "global": "^4.3.2",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "next",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.2.0",
    "serverless-prune-plugin": "^1.1.1",
    "transit-immutable-js": "^0.7.0",
    "transit-js": "^0.8.846"
  }
}
